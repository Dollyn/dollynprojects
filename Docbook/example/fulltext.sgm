<MEMBER><LINK LINKEND="DBRE.ELEM.FUNCSYNOPSISINFO"><SGMLTAG>FuncSynopsisInfo</SGMLTAG></LINK></MEMBER>
<MEMBER><LINK LINKEND="DBRE.ELEM.FUNCTION"><SGMLTAG>Function</SGMLTAG></LINK></MEMBER>
<MEMBER><LINK LINKEND="DBRE.ELEM.PARAMDEF"><SGMLTAG>ParamDef</SGMLTAG></LINK></MEMBER>
<MEMBER><LINK LINKEND="DBRE.ELEM.PARAMETER"><SGMLTAG>Parameter</SGMLTAG></LINK></MEMBER>
<MEMBER><LINK LINKEND="DBRE.ELEM.RETURNVALUE"><SGMLTAG>ReturnValue</SGMLTAG></LINK></MEMBER>
<MEMBER><LINK LINKEND="DBRE.ELEM.VARARGS"><SGMLTAG>VarArgs</SGMLTAG></LINK></MEMBER>
<MEMBER><LINK LINKEND="DBRE.ELEM.VOID"><SGMLTAG>Void</SGMLTAG></LINK></MEMBER>
</SIMPLELIST></PARA>
