<REFSECT1>
<TITLE>Examples</TITLE>
<INFORMALEXAMPLE ROLE="example-source">
<PROGRAMLISTING>&#60;!DOCTYPE table PUBLIC "-//OASIS//DTD DocBook V3.1//EN"&#62;
&#60;table frame=all&#62;&#60;title&#62;Sample Table&#60;/title&#62;
&#60;tgroup cols=5 align=left colsep=1 rowsep=1&#62;
&#60;colspec colname=c1&#62;
&#60;colspec colname=c2&#62;
&#60;colspec colname=c3&#62;
&#60;colspec colnum=5 colname=c5&#62;
&#60;spanspec spanname=hspan namest=c1 nameend=c2 align=center&#62;
&#60;spanspec spanname=bspan namest=c2 nameend=c3 align=center&#62;
&#60;thead&#62;
&#60;row&#62;
  &#60;entry spanname=hspan&#62;Horizontal Span&#60;/entry&#62;
  &#60;entry&#62;a3&#60;/entry&#62;
  &#60;entry&#62;a4&#60;/entry&#62;
  &#60;entry&#62;a5&#60;/entry&#62;
&#60;/row&#62;
&#60;/thead&#62;
&#60;tfoot&#62;
&#60;row&#62;
  &#60;entry&#62;f1&#60;/entry&#62;
  &#60;entry&#62;f2&#60;/entry&#62;
  &#60;entry&#62;f3&#60;/entry&#62;
  &#60;entry&#62;f4&#60;/entry&#62;
  &#60;entry&#62;f5&#60;/entry&#62;
&#60;/row&#62;
&#60;/tfoot&#62;
&#60;tbody&#62;
&#60;row&#62;
  &#60;entry&#62;b1&#60;/entry&#62;
  &#60;entry&#62;b2&#60;/entry&#62;
  &#60;entry&#62;b3&#60;/entry&#62;
  &#60;entry&#62;b4&#60;/entry&#62;
  &#60;entry morerows=1 valign=middle&#62;&#60;para&#62;  &#60;!-- Pernicous Mixed Content --&#62;
  Vertical Span&#60;/para&#62;&#60;/entry&#62;
&#60;/row&#62;
&#60;row&#62;
  &#60;entry&#62;c1&#60;/entry&#62;
  &#60;entry spanname=bspan morerows=1 valign=bottom&#62;Span Both&#60;/entry&#62;
  &#60;entry&#62;c4&#60;/entry&#62;
&#60;/row&#62;
&#60;row&#62;
  &#60;entry&#62;d1&#60;/entry&#62;
  &#60;entry&#62;d4&#60;/entry&#62;
  &#60;entry&#62;d5&#60;/entry&#62;
&#60;/row&#62;
&#60;/tbody&#62;
&#60;/tgroup&#62;
&#60;/table&#62;</PROGRAMLISTING>
</INFORMALEXAMPLE>
<ANCHOR ID="EX.OS.TABLE.1" ROLE="HACK-ex.out.start">
<TABLE FRAME="ALL">
<TITLE>Sample Table</TITLE>
<TGROUP COLS="5" COLSEP="1" ROWSEP="1" ALIGN="LEFT">
<COLSPEC COLNAME="C1">
<COLSPEC COLNAME="C2">
<COLSPEC COLNAME="C3">
<COLSPEC COLNUM="5" COLNAME="C5">
<SPANSPEC NAMEST="C1" NAMEEND="C2" SPANNAME="HSPAN" ALIGN="CENTER">
<SPANSPEC NAMEST="C2" NAMEEND="C3" SPANNAME="BSPAN" ALIGN="CENTER">
<THEAD>
<ROW>
<ENTRY SPANNAME="HSPAN">Horizontal Span</ENTRY>
<ENTRY>a3</ENTRY>
<ENTRY>a4</ENTRY>
<ENTRY>a5</ENTRY>
</ROW>
</THEAD>
<TFOOT>
<ROW>
<ENTRY>f1</ENTRY>
<ENTRY>f2</ENTRY>
<ENTRY>f3</ENTRY>
<ENTRY>f4</ENTRY>
<ENTRY>f5</ENTRY>
</ROW>
</TFOOT>
<TBODY>
<ROW>
<ENTRY>b1</ENTRY>
<ENTRY>b2</ENTRY>
<ENTRY>b3</ENTRY>
<ENTRY>b4</ENTRY>
<ENTRY MOREROWS="1" VALIGN="MIDDLE"><PARA>  
  Vertical Span</PARA></ENTRY>
</ROW>
<ROW>
<ENTRY>c1</ENTRY>
<ENTRY SPANNAME="BSPAN" MOREROWS="1" VALIGN="BOTTOM">Span Both</ENTRY>
<ENTRY>c4</ENTRY>
</ROW>
<ROW>
<ENTRY>d1</ENTRY>
<ENTRY>d4</ENTRY>
<ENTRY>d5</ENTRY>
</ROW>
</TBODY>
</TGROUP>
