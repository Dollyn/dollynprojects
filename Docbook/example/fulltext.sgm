&#60;informalexample&#62;
&#60;programlisting&#62;
sub print_content_model {
    my($self) = shift;
    local($_) = shift;
    local(*FILE) = shift;

    my(@cm) = $self-&#62;format_content_model2($_);
    foreach $_ (@cm) {
        print FILE $self-&#62;make_links($_, 1, 1), "\n";
    }
}
