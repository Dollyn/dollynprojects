<ENTRY NAMEST="C1" NAMEEND="C3" ALIGN="LEFT"><SYNOPSIS>RefNameDiv ::=
(<LINK LINKEND="DBRE.ELEM.REFDESCRIPTOR">RefDescriptor</LINK>?,<LINK LINKEND="DBRE.ELEM.REFNAME">RefName</LINK>+,<LINK LINKEND="DBRE.ELEM.REFPURPOSE">RefPurpose</LINK>,<LINK LINKEND="DBRE.ELEM.REFCLASS">RefClass</LINK>*,
 (<LINK LINKEND="DBRE.ELEM.COMMENT">Comment</LINK>|<LINK LINKEND="DBRE.ELEM.LINK">Link</LINK>|<LINK LINKEND="DBRE.ELEM.OLINK">OLink</LINK>|<LINK LINKEND="DBRE.ELEM.ULINK">ULink</LINK>)*)</SYNOPSIS></ENTRY>
