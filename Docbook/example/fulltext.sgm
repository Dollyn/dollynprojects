<ENTRY NAMEST="C1" NAMEEND="C3" ALIGN="LEFT"><SYNOPSIS>RevNumber ::=
((#PCDATA|<LINK LINKEND="DBRE.ELEM.LINK">Link</LINK>|<LINK LINKEND="DBRE.ELEM.OLINK">OLink</LINK>|<LINK LINKEND="DBRE.ELEM.ULINK">ULink</LINK>|<LINK LINKEND="DBRE.ELEM.EMPHASIS">Emphasis</LINK>|<LINK LINKEND="DBRE.ELEM.TRADEMARK">Trademark</LINK>|<LINK LINKEND="DBRE.ELEM.REPLACEABLE">Replaceable</LINK>|<LINK LINKEND="DBRE.ELEM.COMMENT">Comment</LINK>|
  <LINK LINKEND="DBRE.ELEM.SUBSCRIPT">Subscript</LINK>|<LINK LINKEND="DBRE.ELEM.SUPERSCRIPT">Superscript</LINK>|<LINK LINKEND="DBRE.ELEM.INLINEGRAPHIC">InlineGraphic</LINK>|<LINK LINKEND="DBRE.ELEM.INLINEMEDIAOBJECT">InlineMediaObject</LINK>|<LINK LINKEND="DBRE.ELEM.INDEXTERM">IndexTerm</LINK>)+)</SYNOPSIS></ENTRY>
