</ROW>
<ROW ROWSEP="1" ROLE="cmsynop">
<ENTRY NAMEST="C1" NAMEEND="C3" ALIGN="LEFT"><SYNOPSIS>Revision ::=
(<LINK LINKEND="DBRE.ELEM.REVNUMBER">RevNumber</LINK>,<LINK LINKEND="DBRE.ELEM.DATE">Date</LINK>,<LINK LINKEND="DBRE.ELEM.AUTHORINITIALS">AuthorInitials</LINK>*,<LINK LINKEND="DBRE.ELEM.REVREMARK">RevRemark</LINK>?)</SYNOPSIS></ENTRY>
</ROW>
<ROW ROWSEP="1" ROLE="attrtitle">
