<ENTRY NAMEST="C1" NAMEEND="C3" ALIGN="LEFT"><SYNOPSIS>ParamDef ::=
((#PCDATA|<LINK LINKEND="DBRE.ELEM.REPLACEABLE">Replaceable</LINK>|<LINK LINKEND="DBRE.ELEM.PARAMETER">Parameter</LINK>|<LINK LINKEND="DBRE.ELEM.FUNCPARAMS">FuncParams</LINK>)*)</SYNOPSIS></ENTRY>
</ROW>
<ROW ROWSEP="1" ROLE="attrtitle">
<ENTRY COLSEP="0"><EMPHASIS ROLE="bold">Attributes</EMPHASIS></ENTRY>
