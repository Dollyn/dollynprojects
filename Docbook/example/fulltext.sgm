<REFSECT1>
<TITLE>Examples</TITLE>
<INFORMALEXAMPLE ROLE="example-source">
<PROGRAMLISTING>&#60;!DOCTYPE informaltable PUBLIC "-//OASIS//DTD DocBook V3.1//EN"&#62;
&#60;!-- entrytbl not supported, fake it in descrip.1 --&#62;
&#60;informaltable frame=all&#62;
&#60;tgroup cols=3&#62;
&#60;tbody&#62;
&#60;row&#62;
  &#60;entry&#62;a1&#60;/entry&#62;
  &#60;entry&#62;b1&#60;/entry&#62;
  &#60;entry&#62;c1&#60;/entry&#62;
&#60;/row&#62;
&#60;row&#62;
  &#60;entry&#62;a2&#60;/entry&#62;
  &#60;entrytbl cols=3&#62;
    &#60;tbody&#62;
      &#60;row&#62;
        &#60;entry&#62;b2a1&#60;/entry&#62;
        &#60;entry&#62;b2b1&#60;/entry&#62;
        &#60;entry&#62;b2c1&#60;/entry&#62;
      &#60;/row&#62;
      &#60;row&#62;
        &#60;entry&#62;b2a2&#60;/entry&#62;
        &#60;entry&#62;b2b2&#60;/entry&#62;
        &#60;entry&#62;b2c2&#60;/entry&#62;
      &#60;/row&#62;
      &#60;row&#62;
        &#60;entry&#62;b2a3&#60;/entry&#62;
        &#60;entry&#62;b2b3&#60;/entry&#62;
        &#60;entry&#62;b2c3&#60;/entry&#62;
      &#60;/row&#62;
    &#60;/tbody&#62;
  &#60;/entrytbl&#62;
  &#60;entry&#62;c2&#60;/entry&#62;
&#60;/row&#62;
&#60;row&#62;
  &#60;entry&#62;a3&#60;/entry&#62;
  &#60;entry&#62;b3&#60;/entry&#62;
  &#60;entry&#62;c3&#60;/entry&#62;
&#60;/row&#62;
&#60;/tbody&#62;
&#60;/tgroup&#62;
&#60;/informaltable&#62;</PROGRAMLISTING>
</INFORMALEXAMPLE>
<PARA>The preceding table would look something like this when formatted:</PARA>
<INFORMALTABLE FRAME="ALL">
<TGROUP COLS="5">
<COLSPEC COLNUM="2" COLNAME="C2">
<COLSPEC COLNUM="4" COLNAME="C4">
<SPANSPEC NAMEST="C2" NAMEEND="C4" SPANNAME="HSPAN">
<TBODY>
<ROW>
<ENTRY>a1</ENTRY>
<ENTRY SPANNAME="HSPAN">b1</ENTRY>
<ENTRY>c1</ENTRY>
</ROW>
<ROW>
<ENTRY MOREROWS="2">a2</ENTRY>
<ENTRY>b2a1</ENTRY>
<ENTRY>b2b1</ENTRY>
<ENTRY>b2c1</ENTRY>
<ENTRY MOREROWS="2">c2</ENTRY>
</ROW>
<ROW>
<ENTRY>b2a2</ENTRY>
<ENTRY>b2b2</ENTRY>
<ENTRY>b2c2</ENTRY>
</ROW>
<ROW>
<ENTRY>b2a3</ENTRY>
<ENTRY>b2b3</ENTRY>
<ENTRY>b2c3</ENTRY>
</ROW>
<ROW>
<ENTRY>a3</ENTRY>
<ENTRY SPANNAME="HSPAN">b3</ENTRY>
<ENTRY>c3</ENTRY>
</ROW>
</TBODY>
</TGROUP>
</INFORMALTABLE>
</REFSECT1>
