<ENTRY ALIGN="LEFT" VALIGN="TOP"><PARA><EMPHASIS>Enumeration:</EMPHASIS></PARA><SIMPLELIST>
<MEMBER>None</MEMBER>
<MEMBER>RefEntry</MEMBER>
</SIMPLELIST></ENTRY>
<ENTRY ALIGN="LEFT" VALIGN="TOP">"None"</ENTRY>
</ROW>
